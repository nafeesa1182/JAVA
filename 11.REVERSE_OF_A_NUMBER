import java.util.*;
public class reverse_a_number{
	public static void main(String[] args) {
		Scanner inp=new Scanner(System.in);
		System.out.println("Enter a number:");
		int n=inp.nextInt();
		int rem;
		int rev=0;
		while(n!=0)
		{
			rem=n%10;
			rev=rev*10+rem;
			n/=10;
		}
		System.out.println(rev);
	}
}
