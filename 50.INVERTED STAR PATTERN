package logic_building;
import java.util.Scanner;
public class inverted_pyramid_star_pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner inp=new Scanner(System.in);
		int n=inp.nextInt();
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n-i;j++)
			{
				System.out.print("* ");
			}
			System.out.println();
		}
	}

}
